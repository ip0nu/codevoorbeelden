{% extends 'base.html.twig' %}

{% block body %}
    <h1>UserRelation list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for userRelation in userRelations %}
            <tr>
                <td><a href="{{ path('userRelation_show', { 'id': userRelation.id }) }}">{{ userRelation.id }}</a></td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('userRelation_show', { 'id': userRelation.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('userRelation_edit', { 'id': userRelation.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('userRelation_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
